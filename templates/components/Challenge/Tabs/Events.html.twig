<div{{ attributes }}>
    <table class="challenge-result-event-presenter table table-light table-hover">
        <thead>
        <tr>
            <th scope="col">查詢</th>
            <th scope="col">正確與否</th>
        </tr>
        </thead>
        <tbody>
        {% for event in this.events %}
            <tr key="{{ event.id }}">
                <th scope="row"><code>{{ event.query|u.truncate(50, '…') }}</code></th>
                <td>
                    {% if event.status == constant('\\App\\Entity\\SolutionEventStatus::Failed') %}
                        ❌
                    {% elseif event.status == constant('\\App\\Entity\\SolutionEventStatus::Passed') %}
                        ✅
                    {% else %}
                        ❓
                    {% endif %}
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {{ include('components/Challenge/Tabs/Pagination.html.twig') }}
</div>
